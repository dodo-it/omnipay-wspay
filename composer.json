{
    "name": "dodo-it/omnipay-wspay",
    "description": "WsPay adapter for omnipay",
    "keywords": [
        "omnipay",
        "wspay"
    ],
    "homepage": "https://github.com/dodo-it/omnipay-wspay",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikola Škundrić"
        }
    ],
    "require": {
        "omnipay/common": "3.*"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3",
        "phpro/grumphp": "^0.14"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\WsPay\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
